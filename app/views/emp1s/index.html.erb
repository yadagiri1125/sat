<h1>Listing emp1s</h1>

<table>
  <thead>
    <tr>
      <th>Name1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emp1s.each do |emp1| %>
      <tr>
        <td><%= emp1.name1 %></td>
        <td><%= link_to 'Show', emp1 %></td>
        <td><%= link_to 'Edit', edit_emp1_path(emp1) %></td>
        <td><%= link_to 'Destroy', emp1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Emp1', new_emp1_path %>
